{
  "check-coverage": true,
  "lines": 100,
  "functions": 100,
  "branches": 100,
  "statements": 100,
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "build/",
    "test/**/*"
  ],
  "reporter": [
    "lcov",
    "text"
  ],
  "extension": [
    ".ts"
  ],
  "cache": true,
  "all": true,
  "sourceMap": false,
  "instrument": false
}